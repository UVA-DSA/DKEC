{
  "model": "KAMG",
  "task": "multi_label",
  "dataset": "MIMIC3",
  "root_dir": "/sfs/weka/scratch/zar8jw/EMNLP",

  "train": {
    "learning_rate": 0.001,
    "batch_size": 4,
    "epochs": 200,
    "backbone": "CNN",
    "max_len": 2500,
    "window_size": null,
    "graph": "GCN",
    "loss_type": "BCEWithLogitsLoss",
    "seed": 3407
  },

  "test": {
    "learning_rate": 1e-3,
    "batch_size": 32,
    "epoch": 14,
    "seed": 1234,
    "is_test": false
  },

  "optimizer": {
    "optimizer_type": "Adam",
    "decay_rate": 0,
    "adadelta_decay_rate": 0.95,
    "adadelta_epsilon": 1e-08
  },

  "wandb": {
    "enable": true,
    "entity": "gexueren",
    "project": "DKEC"
  },

  "KAMG": {
    "num_kernels": 200,
    "kernel_size": 10,
    "use_gcn": true,
    "use_multi_graph": true,
    "text_embed_dim": 200,
    "label_embed_dim": 200,
    "gcn_in_features": 200,
    "gcn_hidden_features": 200,
    "gcn_out_features": 200,
    "gcn_dropout": 0
  },

  "fusion": {
    "fusion_type": "concatenation",
    "in_features": 600,
    "out_features": 200
  },

  "DDP": {
    "enable": true
  }
}