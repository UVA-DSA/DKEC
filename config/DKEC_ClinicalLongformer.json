{
  "model": "DKEC",
  "task": "multi_label",
  "dataset": "MIMIC3",
  "root_dir": "/sfs/weka/scratch/zar8jw/EMNLP",

  "train": {
    "learning_rate": 5e-5,
    "batch_size": 8,
    "epochs": 20,
    "dropout": 0.2,
    "max_len": 3584,
    "window_size": 1,
    "graph": "HGT",
    "graph_layer": 1,
    "backbone": "yikuan8/Clinical-Longformer",
    "loss_type": "BCEWithLogitsLoss",
    "seed": 3407
  },

  "test": {
    "learning_rate": 5e-6,
    "batch_size": 8,
    "epoch": 4,
    "seed": 1234,
    "is_test": false
  },

  "optimizer": {
    "optimizer_type": "AdamW",
    "decay_rate": 1e-5,
    "betas": 0.95,
    "eps": 1e-08
  },

  "wandb": {
    "enable": true,
    "entity": "gexueren",
    "project": "DKEC"
  },

  "DKEC": {
    "num_kernels": 200,
    "kernel_size": [5, 7, 9, 11],
    "use_gcn": false,
    "text_embed_dim": 200,
    "label_embed_dim": 200,
    "gcn_in_features": 200,
    "gcn_hidden_features": 200,
    "gcn_out_features": 200,
    "gcn_dropout": 0,
    "hgt_layer": 1
  },

  "DDP": {
    "enable": true
  }
}