{
  "model": "BERT",
  "task": "multi_label",
  "dataset": "MIMIC3",
  "root_dir": "/sfs/weka/scratch/zar8jw/EMNLP",

  "train": {
    "learning_rate": 1e-5,
    "batch_size": 4,
    "epochs": 30,
    "dropout": 0.2,
    "max_len": 1024,
    "window_size": 1,
    "graph": "",
    "graph_layer": null,
    "reduced_dim": null,
    "backbone": "stanford-crfm/BioMedLM",
    "topk": 12,
    "loss_type": "BCEWithLogitsLoss",
    "seed": 3407
  },

  "test": {
    "learning_rate": 1e-5,
    "batch_size": 4,
    "epoch": 6,
    "seed": 3407,
    "is_test": true
  },

  "optimizer": {
    "optimizer_type": "AdamW",
    "decay_rate": 1e-5,
    "betas": 0.95,
    "eps": 1e-08
  },

  "wandb": {
    "enable": true,
    "entity": "gexueren",
    "project": "MIMIC-6668"
  },

  "DKEC": {
    "num_kernels": 200,
    "kernel_size": [5, 7, 9, 11],
    "use_gcn": false,
    "text_embed_dim": 200,
    "label_embed_dim": 200,
    "gcn_in_features": 200,
    "gcn_hidden_features": 200,
    "gcn_out_features": 200,
    "gcn_dropout": 0
  },

  "DDP": {
    "enable": false
  },

  "FSDP": {
    "enable": true,
    "mixed_precision": true,
    "use_fp16": false
  }
}